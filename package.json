{
  "name": "typescript-problem-solving",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --ignore-path .gitignore --check \"**/*.+(js|ts|json)\"",
    "format:fix": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\""
  },
  "keywords": [
    "typescript",
    "typescript-problem-solving",
    "problem-solving"
  ],
  "author": "https://github.com/shahadathhs",
  "license": "ISC",
  "description": "This is a collection of solutions to common problems in typescript",
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "eslint": "^9.26.0",
    "globals": "^16.1.0",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.0"
  }
}
